{
  "name": "流水线插件",
  "description": "针对阿里云流水线发布的一个插件",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "assets/logo.png"
  },
  "host_permissions": [
    "*://flow.aliyun.com/",
    "*://*.aliyun.com/"
  ],
  "permissions": [
    "cookies"
  ]
}